shift + ctrl - 7 : yabai -m window --space 7
shift + ctrl - 8 : yabai -m window --space 8
shift + ctrl - 9 : yabai -m window --space 9
shift + ctrl - 0 : yabai -m window --space 10

shift + ctrl + alt - 1 : yabai -m window --space 11
shift + ctrl + alt - 2 : yabai -m window --space 12
shift + ctrl + alt - 3 : yabai -m window --space 13
shift + ctrl + alt - 4 : yabai -m window --space 14
shift + ctrl + alt - 5 : yabai -m window --space 15
shift + ctrl + alt - 6 : yabai -m window --space 16
shift + ctrl + alt - 7 : yabai -m window --space 17
shift + ctrl + alt - 8 : yabai -m window --space 18
shift + ctrl + alt - 9 : yabai -m window --space 19
shift + ctrl + alt - 0 : yabai -m window --space 20

ctrl - left : yabai -m space --focus prev
ctrl - right : yabai -m space --focus next
ctrl - 1 : yabai -m space --focus 1
ctrl - 2 : yabai -m space --focus 2
ctrl - 3 : yabai -m space --focus 3
